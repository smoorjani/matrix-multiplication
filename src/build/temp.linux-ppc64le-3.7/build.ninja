ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -pthread -B /opt/anaconda/envs/wmlce/compiler_compat -Wl,--sysroot=/ -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -Wstrict-prototypes -fPIC -I/opt/anaconda/envs/wmlce/lib/python3.7/site-packages/torch/include -I/opt/anaconda/envs/wmlce/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/opt/anaconda/envs/wmlce/lib/python3.7/site-packages/torch/include/TH -I/opt/anaconda/envs/wmlce/lib/python3.7/site-packages/torch/include/THC -I/usr/local/cuda/include -I/opt/anaconda/envs/wmlce/include/python3.7m -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=custom_mm -D_GLIBCXX_USE_CXX11_ABI=1 -std=c++14
cuda_cflags = -I/opt/anaconda/envs/wmlce/lib/python3.7/site-packages/torch/include -I/opt/anaconda/envs/wmlce/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/opt/anaconda/envs/wmlce/lib/python3.7/site-packages/torch/include/TH -I/opt/anaconda/envs/wmlce/lib/python3.7/site-packages/torch/include/THC -I/usr/local/cuda/include -I/opt/anaconda/envs/wmlce/include/python3.7m -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=custom_mm -D_GLIBCXX_USE_CXX11_ABI=1 -gencode=arch=compute_70,code=sm_70 -std=c++14
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  command = $nvcc $cuda_cflags -c $in -o $out $cuda_post_cflags



build /home/user/src/kernel/matmul/src/build/temp.linux-ppc64le-3.7/custom_mm.o: compile /home/user/src/kernel/matmul/src/custom_mm.cpp
build /home/user/src/kernel/matmul/src/build/temp.linux-ppc64le-3.7/mm_kernels.o: cuda_compile /home/user/src/kernel/matmul/src/mm_kernels.cu





